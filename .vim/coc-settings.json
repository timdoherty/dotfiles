{
  "coc.preferences.hoverTarget": "float",
  "signature.target": "float",
  "suggest.floatEnable": true,
  "suggest.enablePreview":true,
  "codeLens.enable": false,
  "diagnostic.messageTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "prettier.disableSuccessMessage": true,
  "snippets.extends": {
    "javascriptreact": ["javascript_snippets", "javascript", "javascript_folding"],
    "typescript": ["javascript_snippets", "javascript", "javascriptreact", "javascript_folding"],
    "typescriptreact": ["javascript-jasmine-arrow", "javascript_snippets", "javascript", "javascriptreact", "javascript_folding"]
  },
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "~/.vim/plugged/vim-snippets/UltiSnips"
  ],
  "eslint.options": {
    "configfile": "./.eslintrc.json"
  },
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false
}
